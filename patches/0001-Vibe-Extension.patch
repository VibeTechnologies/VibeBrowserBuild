+ * Get AI configuration
+ */
+async function getAIConfig() {
+  return aiConfig;
+}
+
+// Remove legacy processWithAI and callOpenAIWithTools, replace with new handler
+
+async function processWithAI(tabId, query) {
+  console.log('ü§ñ [AI_AGENT] Processing with ai_agent.js:', query);
+  try {
+    await updateSidePanelStatus(tabId, 'processing', `Processing: "${query}"`);
