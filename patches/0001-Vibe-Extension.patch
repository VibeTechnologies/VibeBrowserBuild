+          console.log('‚úÖ Extension built successfully');
+          resolve();
+        } else {
+          reject(new Error(`Extension build failed with code ${code}`));
+        }
+      });
+      
+      buildProcess.on('error', (error) => {
+        reject(new Error(`Extension build error: ${error.message}`));
+      });
+    });
+  }
+
+  async launchBrowser() {
+    console.log('üåê Launching Chromium with AI Agent...');
+    
+    // First build the extension
+    await this.buildExtension();
+    
+    // Use the same approach as chromium-debug.sh - load extension from dist directory
+    const extensionDistPath = path.join(__dirname, 'chrome/browser/resources/ai/dist');
+    
+    // Same browser args as chromium-debug.sh
+    const browserArgs = [
+      '--user-data-dir=/tmp/chrome_test_profile',
+      '--load-extension=' + extensionDistPath,
+      '--enable-logging',
+      '--v=1',
+      '--disable-web-security',
+      '--disable-features=VizDisplayCompositor',
+      '--enable-experimental-web-platform-features',
+      '--enable-extensions-on-chrome-urls',
+      '--remote-debugging-port=9222',
+      '--no-first-run',
+      '--disable-default-apps',
+      '--new-window',
+      '--no-sandbox',
+      '--disable-dev-shm-usage'
+    ];
+    
+    this.browser = spawn(TEST_CONFIG.browserPath, browserArgs, {
+      stdio: 'pipe',
+      detached: false
+    });
+    
+    let browserStarted = false;
+    let errorOutput = '';
+    let manifestErrors = [];
+    
+    // Monitor browser output
+    this.browser.stdout.on('data', (data) => {
+      const output = data.toString();
+      console.log('ü§ñ Browser stdout:', output.trim());
+      if (output.includes('Extension') || output.includes('loaded')) {
+        browserStarted = true;
+      }
+    });
+    
+    this.browser.stderr.on('data', (data) => {
+      const error = data.toString();
+      errorOutput += error;
+      console.log('üîç Browser stderr:', error.trim());
+      
+      // Check for component extension manifest errors
+      if (error.includes('Failed to parse extension manifest')) {
+        manifestErrors.push(error);
+        console.log('‚ùå Component extension manifest error detected');
+      }
+    });
+    
+    this.browser.on('error', (error) => {
+      throw new Error(`Browser process error: ${error.message}`);
+    });
+    
+    this.browser.on('exit', (code) => {
+      if (code !== 0) {
+        throw new Error(`Browser exited with code ${code}. Error: ${errorOutput}`);
+      }
+    });
+    
+    // Wait for browser to start and validate
+    await this.waitFor(12000);
+    
+    // Check for manifest errors before proceeding
+    if (manifestErrors.length > 0) {
+      console.log('‚ùå Component extension manifest parsing failed:');
+      manifestErrors.forEach(err => console.log('  ' + err.trim()));
+      throw new Error('Component extension manifest parsing failed. Extension will not load.');
+    }
+    
+    // Validate browser is running
+    const isRunning = await this.validateBrowserRunning();
+    if (!isRunning) {
+      console.log('‚ö†Ô∏è  Browser validation failed, but continuing with test');
+      // Don't throw error - continue with test
+    }
+    
+    // Check if AI extension loaded properly
+    const extensionLoaded = await this.checkAIExtensionLoaded();
+    if (!extensionLoaded) {
+      console.log('‚ö†Ô∏è  AI extension not loaded - this will cause test failures');
+    }
+    
+    // Ensure browser is focused and ready
+    await this.ensureBrowserFocused();
+    
+    console.log('‚úÖ Browser launched and validated');
+  }
+  
+  async validateBrowserRunning() {
+    console.log('üîç Validating browser is running...');
+    
+    try {
+      // Check if browser process is still running
+      if (this.browser.killed || this.browser.exitCode !== null) {
+        console.log('‚ùå Browser process was killed or exited');
+        return false;
+      }
+      
+      // Try multiple times to check if Chrome DevTools port is responding
+      for (let attempt = 1; attempt <= 8; attempt++) {
+        console.log(`üîç DevTools check attempt ${attempt}/8...`);
+        
+        const isResponding = await new Promise((resolve) => {
+          const checkDevTools = spawn('curl', ['-s', '-m', '5', 'http://localhost:9222/json/version']);
+          let output = '';
+          
+          checkDevTools.stdout.on('data', (data) => {
+            output += data.toString();
+          });
+          
+          checkDevTools.on('close', (code) => {
+            if (code === 0 && output.includes('Chrome')) {
+              console.log('‚úÖ Browser DevTools responding');
+              resolve(true);
+            } else {
+              console.log(`‚ùå DevTools check failed (code: ${code})`);
+              resolve(false);
+            }
+          });
+          
+          checkDevTools.on('error', () => {
+            resolve(false);
+          });
+          
+          // Timeout after 5 seconds
+          setTimeout(() => {
+            checkDevTools.kill();
+            resolve(false);
+          }, 5000);
+        });
+        
+        if (isResponding) {
+          return true;
+        }
+        
+        // Wait before next attempt (longer delay)
+        await this.waitFor(3000);
+      }
+      
+      console.log('‚ùå Browser validation failed after 8 attempts');
+      return false;
+      
+    } catch (error) {
+      console.log('‚ùå Browser validation failed:', error.message);
+      return false;
+    }
+  }
+  
+  async checkAIExtensionLoaded() {
+    console.log('üîç Checking AI extension loaded...');
+    
+    try {
+      // Check if extension appears in browser tabs or DevTools
+      const tabs = await this.getChromeTabs();
+      
+      // Look for AI extension-related tabs
+      const aiExtensionTabs = tabs.filter(tab => 
