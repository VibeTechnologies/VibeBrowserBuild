+import { AIConfigManager } from './ai_config_manager.js';
+import { isNaturalLanguageQuery, extractQueryFromUrl } from './query_utils.js';
+
+/**
+ * Vibe AI Agent Background Service Worker
+ * Handles natural language query processing and browser automation
+ */
+
+console.log('ü§ñ Vibe AI Agent background script starting...');
+
+// Side panel support - auto-open on extension load
+chrome.sidePanel
+  .setPanelBehavior({ openPanelOnActionClick: true })
+  .catch((error) => console.error('Side panel setup error:', error));
+
+// No auto-opening - users will open side panel manually
+
+// AI Agent state
+const configManager = new AIConfigManager();
+let aiConfig = configManager.getConfig();
+
+// Handle keyboard commands
+chrome.commands.onCommand.addListener((command) => {
+  console.log('‚å®Ô∏è Command received:', command);
+  
+  if (command === 'toggle-panel') {
+    chrome.tabs.query({ active: true, currentWindow: true }, (tabs) => {
+      if (tabs[0]) {
+        chrome.sidePanel.open({ tabId: tabs[0].id });
+      }
+    });
+  }
+});
+
+// Handle messages from side panel and content scripts
+chrome.runtime.onMessage.addListener((message, sender, sendResponse) => {
+  console.log('üì® Background received message:', message);
+  
+  if (message.type === 'processCommand') {
+    handleSidePanelCommand(message, sendResponse);
+    return true; // Keep the message channel open for async response
+  }
+  
+  if (message.type === 'domAnalysisResult') {
+    // Store DOM structure for next AI call
+    console.log('üìä Received DOM analysis from content script:', message.data);
+    // You can store this in a global variable or pass it to the next AI call
+    return true;
+  }
+
+  if (message.type === 'askUserQuestion') {
+    // Forward the question to the side panel and wait for a response
+    chrome.runtime.sendMessage({
+      type: 'displayQuestion',
+      question: message.question,
+      context: message.context
+    }, (response) => {
+      sendResponse(response);
+    });
+    return true; // Keep the message channel open for async response
+  }
+
+  if (message.type === 'showUserOptions') {
+    // Forward options to the side panel and wait for user selection
+    chrome.runtime.sendMessage({
+      type: 'displayOptions',
+      message: message.message,
+      options: message.options,
+      context: message.context
+    }, (response) => {
+      sendResponse(response);
+    });
+    return true; // Keep the message channel open for async response
+  }
+
+  if (message.type === 'requestConfirmation') {
+    // Forward confirmation request to the side panel and wait for user confirmation
+    chrome.runtime.sendMessage({
+      type: 'displayConfirmation',
+      action: message.action,
+      details: message.details,
+      price: message.price
+    }, (response) => {
+      sendResponse(response);
+    });
+    return true; // Keep the message channel open for async response
+  }
+  
+  return false;
+});
+
+// Handle side panel commands
+async function handleSidePanelCommand(message, sendResponse) {
+  const { command, source } = message;
+  
+  try {
+    console.log('ü§ñ Processing side panel command:', command);
+    
+    // Get current tab
+    const [tab] = await chrome.tabs.query({ active: true, currentWindow: true });
+    if (!tab) {
+      throw new Error('No active tab found');
+    }
+    
+    // Process with AI (same logic as navigation interception)
+    const result = await processWithAI(tab.id, command);
+    
+    sendResponse({
+      success: true,
+      response: `‚úÖ Executed: ${command}`,
+      result: result
+    });
+    
+  } catch (error) {
+    console.error('‚ùå Side panel command error:', error);
+    sendResponse({
+      success: false,
+      error: error.message
+    });
+  }
+}
+
+// Listen for tab navigation to intercept natural language queries
+chrome.webNavigation.onBeforeNavigate.addListener(async (details) => {
+  // Only process main frame navigations
+  if (details.frameId !== 0) return;
+  
+  const url = details.url;
+  console.log('Raw URL from onBeforeNavigate:', url);
+  console.log('üîç [NAVIGATION] Navigation detected:', url);
+  
+  // Check if this looks like a natural language query
+  const isNaturalLanguage = isNaturalLanguageQuery(url);
+  console.log('üß† [NAVIGATION] Natural language detection result:', isNaturalLanguage);
+  
+  if (isNaturalLanguage) {
+    console.log('üéØ [NAVIGATION] Natural language query detected, intercepting navigation');
+    
+    // Extract the query from the URL
+    const extractedQuery = extractQueryFromUrl(url);
+    console.log('üìù [NAVIGATION] Extracted query:', extractedQuery);
+    
+    // IMPORTANT: Cancel the original navigation by navigating to about:blank
+    // This prevents going to Google search results
+    console.log('üõë [NAVIGATION] Canceling original navigation');
