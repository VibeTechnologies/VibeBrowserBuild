+    
+    await this.executeAppleScript(script);
+    await this.waitFor(2000); // Wait for focus to take effect
+    
+    console.log('‚úÖ Browser focused');
+  }
+
+  async runTestCase(testCase) {
+    console.log(`\nüß™ Running Test Case: ${testCase.id}`);
+    console.log(`Query: "${testCase.query}"`);
+    console.log('‚îÄ'.repeat(60));
+    
+    const testStart = Date.now();
+    const result = {
+      id: testCase.id,
+      query: testCase.query,
+      startTime: testStart,
+      status: 'running',
+      steps: [],
+      screenshots: [],
+      errors: []
+    };
+    
+    try {
+      // Step 1: Input query in address bar
+      await this.inputQueryInAddressBar(testCase.query);
+      result.steps.push('‚úÖ Query input in address bar');
+      
+      // Step 2: Wait for AI agent to load - FAIL FAST if not loaded
+      const agentLoaded = await this.waitForAIAgent();
+      if (!agentLoaded) {
+        throw new Error('CRITICAL: AI Extension not loaded - cannot continue test. Extension must be properly integrated and loaded.');
+      }
+      result.steps.push('‚úÖ AI Agent loaded');
+      
+      // Step 3: Check if query was intercepted - FAIL FAST if not intercepted
+      const queryIntercepted = await this.checkQueryInterception(testCase.query);
+      if (!queryIntercepted) {
+        throw new Error('CRITICAL: Query not intercepted by AI agent - omnibox interception not working. Test cannot continue.');
