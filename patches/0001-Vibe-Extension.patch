+      throw new Error('OpenAI API key not configured. Please set your API key in background.js');
+    }
+    // Use aiAgent to process the user request, passing tabId
+    const result = await aiAgent.processUserRequest({
+      user_request: query,
+      tabId,
+      config,
+    });
+    // Report tool results and final reasoning to side panel
+    for (const toolResult of result.toolResults) {
+      await updateSidePanelProgress(tabId, `${toolResult.name}: ${toolResult.result}`);
+    }
+    await updateSidePanelProgress(tabId, result.reasoning || 'Task completed successfully');
+  } catch (error) {
+    console.error('‚ùå AI processing failed:', error);
