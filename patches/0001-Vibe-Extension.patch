+    
+    if (aiAgentLogs.length > 0) {
+      console.log(`‚úÖ Found ${aiAgentLogs.length} AI Agent log entries`);
+      aiAgentLogs.forEach(log => {
+        console.log(`  - ${log.level}: ${log.message}`);
+      });
+    } else {
+      console.log('‚ö†Ô∏è  No AI Agent logs found in browser console');
