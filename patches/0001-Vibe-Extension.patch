+// Copyright 2024 The Chromium Authors
+// Use of this source code is governed by a BSD-style license that can be
+// found in the LICENSE file.
+
+// API Key Management UI for Vibe AI
+class APIKeyManager {
+  private keyManagerInterface: any;
+  private container: HTMLElement;
+
+  constructor(container: HTMLElement) {
+    this.container = container;
+    this.initializeInterface();
+    this.createUI();
+  }
+
+  private async initializeInterface() {
+    try {
+      this.keyManagerInterface = await (window as any).chrome?.mojom?.vibeAI?.getKeyManagerInterface();
+    } catch (error) {
+      console.error('Failed to initialize API key manager:', error);
+    }
+  }
+
+  private createUI() {
+    this.container.innerHTML = `
+      <div class="api-key-manager">
+        <h2>AI API Configuration</h2>
+        <p>Configure API keys for external AI providers to enable advanced features.</p>
+        
+        <div class="api-providers">
+          <div class="provider-section" data-provider="openai">
+            <h3>
+              <img src="data:image/svg+xml;base64,..." alt="OpenAI" class="provider-icon">
+              OpenAI
+            </h3>
+            <div class="api-key-input">
+              <input type="password" placeholder="Enter OpenAI API key" class="api-key-field">
+              <button class="save-key-btn">Save</button>
+              <button class="test-key-btn">Test</button>
+              <button class="remove-key-btn">Remove</button>
+            </div>
+            <div class="key-status"></div>
+            <p class="provider-info">
+              Supports GPT-4o, GPT-4o mini, and other OpenAI models.
+              <a href="https://platform.openai.com/api-keys" target="_blank">Get API key</a>
+            </p>
+          </div>
+          
+          <div class="provider-section" data-provider="claude">
+            <h3>
+              <img src="data:image/svg+xml;base64,..." alt="Claude" class="provider-icon">
+              Anthropic Claude
+            </h3>
+            <div class="api-key-input">
+              <input type="password" placeholder="Enter Claude API key" class="api-key-field">
+              <button class="save-key-btn">Save</button>
+              <button class="test-key-btn">Test</button>
+              <button class="remove-key-btn">Remove</button>
+            </div>
+            <div class="key-status"></div>
+            <p class="provider-info">
+              Supports Claude 3.5 Sonnet, Haiku, and other Anthropic models.
+              <a href="https://console.anthropic.com/" target="_blank">Get API key</a>
+            </p>
+          </div>
+          
+          <div class="provider-section" data-provider="gemini">
+            <h3>
+              <img src="data:image/svg+xml;base64,..." alt="Gemini" class="provider-icon">
+              Google Gemini
+            </h3>
+            <div class="api-key-input">
+              <input type="password" placeholder="Enter Gemini API key" class="api-key-field">
+              <button class="save-key-btn">Save</button>
+              <button class="test-key-btn">Test</button>
+              <button class="remove-key-btn">Remove</button>
+            </div>
+            <div class="key-status"></div>
+            <p class="provider-info">
+              Supports Gemini 1.5 Pro, Flash, and other Google models.
+              <a href="https://makersuite.google.com/app/apikey" target="_blank">Get API key</a>
+            </p>
+          </div>
+        </div>
+        
+        <div class="ai-settings">
+          <h3>AI Behavior Settings</h3>
+          
+          <div class="setting-group">
+            <label>Default AI Provider:</label>
+            <select class="default-provider-select">
+              <option value="builtin">Built-in Chromium AI</option>
+              <option value="openai">OpenAI</option>
+              <option value="claude">Anthropic Claude</option>
+              <option value="gemini">Google Gemini</option>
+            </select>
+          </div>
+          
+          <div class="setting-group">
+            <label>Response Temperature:</label>
+            <input type="range" min="0" max="1" step="0.1" value="0.7" class="temperature-slider">
+            <span class="temperature-value">0.7</span>
+          </div>
+          
+          <div class="setting-group">
+            <label>Max Response Length:</label>
+            <select class="max-tokens-select">
+              <option value="500">Short (500 tokens)</option>
+              <option value="1000" selected>Medium (1000 tokens)</option>
+              <option value="2000">Long (2000 tokens)</option>
+              <option value="4000">Very Long (4000 tokens)</option>
+            </select>
+          </div>
+        </div>
+      </div>
+    `;
+
+    this.attachEventListeners();
+    this.loadCurrentSettings();
+  }
+
+  private attachEventListeners() {
+    // Save key buttons
+    this.container.querySelectorAll('.save-key-btn').forEach(btn => {
+      btn.addEventListener('click', (e) => this.handleSaveKey(e));
+    });
+
+    // Test key buttons
+    this.container.querySelectorAll('.test-key-btn').forEach(btn => {
+      btn.addEventListener('click', (e) => this.handleTestKey(e));
+    });
+
+    // Remove key buttons
+    this.container.querySelectorAll('.remove-key-btn').forEach(btn => {
+      btn.addEventListener('click', (e) => this.handleRemoveKey(e));
+    });
+
+    // Settings changes
+    const temperatureSlider = this.container.querySelector('.temperature-slider') as HTMLInputElement;
+    const temperatureValue = this.container.querySelector('.temperature-value');
+    
+    temperatureSlider?.addEventListener('input', () => {
+      if (temperatureValue) {
+        temperatureValue.textContent = temperatureSlider.value;
+      }
+    });
+
+    // Auto-save settings
+    this.container.querySelectorAll('select, input[type="range"]').forEach(input => {
+      input.addEventListener('change', () => this.saveSettings());
+    });
+  }
+
+  private async handleSaveKey(event: Event) {
+    const button = event.target as HTMLButtonElement;
+    const section = button.closest('.provider-section') as HTMLElement;
+    const provider = section.dataset['provider']!;
+    const input = section.querySelector('.api-key-field') as HTMLInputElement;
+    const status = section.querySelector('.key-status') as HTMLElement;
+
+    if (!input.value.trim()) {
+      this.showStatus(status, 'Please enter an API key', 'error');
+      return;
+    }
+
+    try {
+      button.disabled = true;
+      button.textContent = 'Saving...';
+
+      const success = await this.keyManagerInterface.setApiKey(provider, input.value.trim());
+      
+      if (success) {
+        this.showStatus(status, 'API key saved successfully', 'success');
+        input.value = ''; // Clear the input for security
+      } else {
+        this.showStatus(status, 'Failed to save API key', 'error');
+      }
+    } catch (error) {
+      this.showStatus(status, `Error: ${(error as Error).message}`, 'error');
+    } finally {
+      button.disabled = false;
+      button.textContent = 'Save';
+    }
+  }
+
+  private async handleTestKey(event: Event) {
+    const button = event.target as HTMLButtonElement;
+    const section = button.closest('.provider-section') as HTMLElement;
+    const provider = section.dataset['provider']!;
+    const status = section.querySelector('.key-status') as HTMLElement;
+
+    try {
+      button.disabled = true;
+      button.textContent = 'Testing...';
+
+      const { is_valid, error_message } = await this.keyManagerInterface.validateApiKey(provider);
+      
+      if (is_valid) {
+        this.showStatus(status, 'API key is valid', 'success');
+      } else {
+        this.showStatus(status, error_message || 'API key validation failed', 'error');
+      }
+    } catch (error) {
+      this.showStatus(status, `Test failed: ${(error as Error).message}`, 'error');
+    } finally {
+      button.disabled = false;
+      button.textContent = 'Test';
+    }
+  }
+
+  private async handleRemoveKey(event: Event) {
+    const button = event.target as HTMLButtonElement;
+    const section = button.closest('.provider-section') as HTMLElement;
+    const provider = section.dataset['provider']!;
+    const status = section.querySelector('.key-status') as HTMLElement;
+
+    if (!confirm(`Remove API key for ${provider}?`)) {
+      return;
+    }
+
+    try {
+      const success = await this.keyManagerInterface.removeApiKey(provider);
+      
+      if (success) {
+        this.showStatus(status, 'API key removed', 'info');
+      } else {
+        this.showStatus(status, 'Failed to remove API key', 'error');
+      }
+    } catch (error) {
+      this.showStatus(status, `Error: ${(error as Error).message}`, 'error');
+    }
+  }
+
+  private async loadCurrentSettings() {
+    // Check which providers have API keys configured
+    const providers = ['openai', 'claude', 'gemini'];
+    
+    for (const provider of providers) {
+      try {
+        const hasKey = await this.keyManagerInterface.hasApiKey(provider);
+        const section = this.container.querySelector(`[data-provider="${provider}"]`) as HTMLElement;
+        const status = section.querySelector('.key-status') as HTMLElement;
+        
+        if (hasKey) {
+          this.showStatus(status, 'API key configured', 'success');
+        } else {
+          this.showStatus(status, 'No API key configured', 'info');
+        }
+      } catch (error) {
+        console.error(`Error checking API key for ${provider}:`, error);
+      }
+    }
+  }
+
+  private showStatus(element: HTMLElement, message: string, type: 'success' | 'error' | 'info') {
+    element.className = `key-status ${type}`;
+    element.textContent = message;
+    
+    // Auto-hide success/info messages after 3 seconds
+    if (type !== 'error') {
+      setTimeout(() => {
+        element.textContent = '';
+        element.className = 'key-status';
+      }, 3000);
+    }
+  }
+
+  private saveSettings() {
+    // Save UI settings to preferences
+    const settings = {
+      defaultProvider: (this.container.querySelector('.default-provider-select') as HTMLSelectElement)?.value,
+      temperature: parseFloat((this.container.querySelector('.temperature-slider') as HTMLInputElement)?.value || '0.7'),
+      maxTokens: parseInt((this.container.querySelector('.max-tokens-select') as HTMLSelectElement)?.value || '1000')
+    };
+
+    // TODO: Save to Chromium preferences via Mojo interface
+    console.log('Saving AI settings:', settings);
+  }
+}
+
+// CSS styles for the API key manager
+const styles = `
+  .api-key-manager {
+    max-width: 800px;
+    margin: 0 auto;
+    padding: 20px;
+    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
+  }
+
+  .provider-section {
+    border: 1px solid #e0e0e0;
+    border-radius: 8px;
+    padding: 20px;
+    margin-bottom: 20px;
+    background: #fafafa;
+  }
+
+  .provider-section h3 {
+    display: flex;
+    align-items: center;
+    margin: 0 0 15px 0;
+    font-size: 18px;
+  }
+
+  .provider-icon {
+    width: 24px;
+    height: 24px;
+    margin-right: 10px;
+  }
+
+  .api-key-input {
+    display: flex;
+    gap: 10px;
+    margin-bottom: 10px;
+  }
+
+  .api-key-field {
+    flex: 1;
+    padding: 8px 12px;
+    border: 1px solid #ccc;
+    border-radius: 4px;
+    font-family: monospace;
+  }
+
+  .api-key-input button {
+    padding: 8px 16px;
+    border: none;
+    border-radius: 4px;
+    cursor: pointer;
+    font-weight: 500;
+  }
+
+  .save-key-btn {
+    background: #1976d2;
+    color: white;
+  }
+
+  .test-key-btn {
+    background: #388e3c;
+    color: white;
+  }
+
+  .remove-key-btn {
+    background: #d32f2f;
+    color: white;
+  }
+
+  .key-status {
+    font-size: 14px;
+    margin-bottom: 10px;
+    padding: 5px 0;
+  }
+
+  .key-status.success {
+    color: #388e3c;
+  }
+
+  .key-status.error {
+    color: #d32f2f;
+  }
+
+  .key-status.info {
+    color: #1976d2;
+  }
+
+  .provider-info {
+    font-size: 14px;
+    color: #666;
+    margin: 0;
+  }
+
+  .ai-settings {
+    border-top: 2px solid #e0e0e0;
+    padding-top: 30px;
+    margin-top: 30px;
+  }
+
+  .setting-group {
+    display: flex;
+    align-items: center;
+    margin-bottom: 15px;
+    gap: 15px;
+  }
+
+  .setting-group label {
+    min-width: 150px;
+    font-weight: 500;
+  }
+
+  .setting-group select,
+  .setting-group input {
+    padding: 6px 10px;
+    border: 1px solid #ccc;
+    border-radius: 4px;
+  }
+
+  .temperature-slider {
+    width: 200px;
+  }
+
+  .temperature-value {
+    font-weight: 500;
+    min-width: 30px;
+  }
+`;
+
+// Inject styles
+if (!document.querySelector('#vibe-ai-styles')) {
+  const styleSheet = document.createElement('style');
+  styleSheet.id = 'vibe-ai-styles';
+  styleSheet.textContent = styles;
+  document.head.appendChild(styleSheet);
+}
+
+// Export for use in settings pages
+export { APIKeyManager };
