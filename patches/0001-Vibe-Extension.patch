+bool IsValidURL(const std::string& input) {
+  GURL url(input);
+  return url.is_valid() && (url.has_scheme() || input.find('.') != std::string::npos);
+}
+
+bool ContainsQuestionWords(const std::string& input) {
+  std::string lower_input = base::ToLowerASCII(input);
+  const std::vector<std::string> question_words = {
+    "what", "how", "why", "when", "where", "who", "which", "can", "could", 
+    "should", "would", "do", "does", "did", "is", "are", "was", "were"
+  };
+  
+  for (const auto& word : question_words) {
+    if (lower_input.find(word + " ") == 0 || 
+        lower_input.find(" " + word + " ") != std::string::npos) {
+      return true;
+    }
+  }
+  return false;
+}
+
+}  // namespace
+
+VibeAIService::VibeAIService(content::BrowserContext* browser_context)
+    : browser_context_(browser_context) {
+  // Initialize with environment variable if available
+  const char* openai_key = std::getenv("OPENAI_API_KEY");
+  if (openai_key && strlen(openai_key) > 0) {
+    llm_config_.api_key = std::string(openai_key);
+    llm_config_.backend = VibeLLMBackend::kOpenAI;
+    LOG(INFO) << "Loaded OPENAI_API_KEY from environment";
+  }
+  
+  // Initialize browser tools
+  InitializeTools();
+}
+
+VibeAIService::~VibeAIService() = default;
+
+void VibeAIService::InitializeTools() {
+  // TODO: Initialize tools with proper dependencies
+  // These tools need proper initialization with browser context and factories
+  // For now, leaving them uninitialized to avoid compilation errors
+  // navigation_tool_ = std::make_unique<BrowserNavigationTool>(browser);
+  // dom_tool_ = std::make_unique<DOMManipulationTool>();
+  // search_tool_ = std::make_unique<SearchTool>(url_loader_factory);
+}
+
+void VibeAIService::SetLLMConfig(const VibeLLMConfig& config) {
+  llm_config_ = config;
+}
+
+// BrowserNavigationTool* VibeAIService::GetNavigationTool() {
+//   return navigation_tool_.get();
+// }
+
+// DOMManipulationTool* VibeAIService::GetDOMTool() {
+//   return dom_tool_.get();
+// }
+
+// SearchTool* VibeAIService::GetSearchTool() {
+//   return search_tool_.get();
+// }
+
+bool VibeAIService::IsNaturalLanguagePrompt(const std::string& input) {
+  // Check if it's clearly a URL
+  if (IsValidURL(input)) {
+    return false;
+  }
+  
+  // Check for question words or command words
+  if (ContainsQuestionWords(input)) {
+    return true;
+  }
+  
+  // Check for sentence-like structure (multiple words with spaces)
+  std::vector<std::string> words = base::SplitString(
+      input, " ", base::TRIM_WHITESPACE, base::SPLIT_WANT_NONEMPTY);
+  
+  return words.size() >= 3;  // 3+ words likely indicates natural language
+}
