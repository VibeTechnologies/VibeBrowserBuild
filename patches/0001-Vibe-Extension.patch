+      // FAIL FAST: If this is a critical error, stop all remaining tests
+      if (error.message.includes('CRITICAL:')) {
+        console.log('üõë CRITICAL ERROR DETECTED - STOPPING ALL TESTS');
+        throw error; // Re-throw to stop the entire test suite
+      }
+    }
+    
+    this.testResults.push(result);
+  }
+
+  async inputQueryInAddressBar(query) {
+    console.log(`üìù Inputting query: "${query}"`);
+    
+    // First validate browser is still running
+    const isRunning = await this.validateBrowserRunning();
+    if (!isRunning) {
+      throw new Error('Browser is not running - cannot input query');
+    }
+    
+    // Ensure browser is focused
+    await this.ensureBrowserFocused();
+    
+    // Use AppleScript to interact with browser
