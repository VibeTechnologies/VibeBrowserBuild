+    const script = `
+      tell application "Chromium"
+        activate
+        delay 1
+        tell application "System Events"
+          keystroke "l" using {command down}
+          delay 1
+          keystroke "a" using {command down}
+          delay 0.5
+          keystroke "${query}"
+          delay 1
+          keystroke return
+        end tell
+        delay 2
