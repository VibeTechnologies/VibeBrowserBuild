+      
+    } catch (error) {
+      console.log('‚ùå Failed to check AI extension:', error.message);
+      return false;
+    }
+  }
+
+  async ensureBrowserFocused() {
+    console.log('üéØ Ensuring browser is focused...');
+    
+    const script = `
+      tell application "Chromium"
+        activate
+        set frontmost to true
+      end tell
+      
+      tell application "System Events"
+        tell process "Chromium"
+          set frontmost to true
+        end tell
