+    });
+    console.log('üìà Progress updated:', progress);
+  } catch (error) {
+    console.error('‚ùå Failed to update progress:', error);
+  }
+}
+
+/**
+ * Send error message to side panel
+ */
+async function sendSidePanelError(tabId, errorMessage) {
+  try {
+    await chrome.runtime.sendMessage({
+      type: 'commandError',
+      error: errorMessage
+    });
+    console.log('‚ùå Error sent to side panel:', errorMessage);
+  } catch (error) {
+    console.error('‚ùå Failed to send error to side panel:', error);
+  }
+}
+
+// Remove legacy configuration change listener
+
+console.log('‚úÖ [INIT] Vibe AI Agent background script initialized:', {
+  timestamp: new Date().toISOString(),
+  version: '2.0.0-langchain',
+  aiConfig: aiConfig
+});
+
+// Remove legacy aiAgent initialization
+
+let aiAgent = null;
+
+(async () => {
+  aiAgent = (await import('./ai_agent.js')).default;
+  console.log('‚úÖ [INIT] ai_agent.js loaded:', aiAgent);
+})();
