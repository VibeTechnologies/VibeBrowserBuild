--- /dev/null
+++ b/chrome/browser/resources/ai/run_npm_build.py
@@ -0,0 +1,35 @@
+import subprocess
+import sys
+import os
+
+# Get the directory of the current script
+script_dir = os.path.dirname(os.path.abspath(__file__))
+# The npm project directory is the same as the script's directory
+npm_project_dir = script_dir
+
+print(f"Running npm build in: {npm_project_dir}")
+
+try:
+    # Run 'npm install' first to ensure dependencies are met
+    install_command = ["npm", "install"]
+    install_result = subprocess.run(install_command, cwd=npm_project_dir, check=True, capture_output=True, text=True)
+    print(f"npm install stdout:\n{install_result.stdout}")
+    if install_result.stderr:
+        print(f"npm install stderr:\n{install_result.stderr}")
+
+    # Run 'npm run build'
+    build_command = ["node", "build.cjs"]
+    build_result = subprocess.run(build_command, cwd=npm_project_dir, check=True, capture_output=True, text=True)
+    print(f"node build.cjs stdout:\n{build_result.stdout}")
+    if build_result.stderr:
+        print(f"node build.cjs stderr:\n{build_result.stderr}")
+
+    sys.exit(0)
+except subprocess.CalledProcessError as e:
+    print(f"Error during npm build: {e}", file=sys.stderr)
+    print(f"stdout:\n{e.stdout}", file=sys.stderr)
+    print(f"stderr:\n{e.stderr}", file=sys.stderr)
+    sys.exit(e.returncode)
+except FileNotFoundError:
+    print("Error: npm command not found. Please ensure Node.js and npm are installed and in your PATH.", file=sys.stderr)
+    sys.exit(1)
