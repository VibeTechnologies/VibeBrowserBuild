+
+// Load environment variables from .env file
+async function loadEnvFile() {
+  try {
+    const envFile = await fs.readFile('.env', 'utf8');
+    const envLines = envFile.split('\n').filter(line => line.trim() && !line.startsWith('#'));
+    for (const line of envLines) {
+      if (line.includes('=')) {
+        // Handle export VARNAME=value format
+        let cleanLine = line.replace(/^export\s+/, '').trim();
+        const [key, ...valueParts] = cleanLine.split('=');
+        const value = valueParts.join('=').trim();
+        if (key.trim() && value) {
+          process.env[key.trim()] = value;
+          console.log(`üîß Loaded env var: ${key.trim()}=${value.substring(0, 10)}...`);
+        }
+      }
+    }
+  } catch (error) {
+    console.log('‚ö†Ô∏è  Could not load .env file:', error.message);
+  }
+}
+
+// Test configuration
+const TEST_CONFIG = {
+  browserPath: './out/Debug/Chromium.app/Contents/MacOS/Chromium',
+  extensionPath: './chrome/browser/resources/ai',
+  screenshotDir: './test_screenshots',
+  testTimeout: 60000, // 1 minute per test
