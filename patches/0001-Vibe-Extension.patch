+
+  async checkAPIKey() {
+    if (process.env.OPENAI_API_KEY) {
+      console.log('‚úÖ OpenAI API key configured');
+    } else {
+      console.log('‚ö†Ô∏è  No OpenAI API key - tests may fail');
+    }
+  }
+
+  async buildExtension() {
+    console.log('üî® Building AI extension...');
+    
+    const aiPanelDir = path.join(__dirname, 'chrome/browser/resources/ai');
+    
+    // Build the extension using npm (same as chromium-debug.sh)
+    const buildProcess = spawn('npm', ['run', 'build'], {
+      cwd: aiPanelDir,
+      stdio: 'inherit'
