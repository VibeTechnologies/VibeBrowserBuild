+      query: 'book flight SFO JFK on Aug 1',
+      expectedSteps: [
+        'AI Agent loads',
+        'Side panel opens',
+        'OpenAI API calls made',
+        'Navigate to flights.google.com',
+        'Fill SFO ‚Üí JFK, Aug 1',
+        'Search executed'
+      ],
+      expectedSites: ['flights.google.com', 'google.com/travel/flights'],
+      success: 'Flight search results for SFO-JFK on Aug 1'
+    },
+    {
+      id: 'hotel_nyc_budget',
+      query: 'book a hotel in New York Aug 1 for a week under 100$ a night',
+      expectedSteps: [
+        'AI Agent loads',
+        'Side panel opens',
+        'OpenAI API calls made',
+        'Navigate to booking.com or hotels.com',
+        'Fill NYC, Aug 1-8, under $100',
+        'Search executed'
+      ],
+      expectedSites: ['booking.com', 'hotels.com', 'agoda.com'],
+      success: 'Hotel search results in NYC under $100/night'
+    },
+    {
+      id: 'pixel9_case_aliexpress',
+      query: 'Buy a phone case for Pixel 9 on aliexpress',
+      expectedSteps: [
+        'AI Agent loads',
+        'Side panel opens',
+        'OpenAI API calls made',
+        'Navigate to aliexpress.com',
+        'Search "Pixel 9 case"',
+        'Filter by ratings'
+      ],
+      expectedSites: ['aliexpress.com'],
+      success: 'Pixel 9 case search results with ratings'
+    }
+  ]
+};
+
+class AIAgentBrowserTest {
+  constructor() {
+    this.browser = null;
+    this.testResults = [];
+    this.startTime = Date.now();
+    this.screenshots = [];
+  }
+
+  async run() {
+    console.log('üöÄ AI Agent Browser Test Suite');
+    console.log('==============================');
+    
+    try {
+      // Setup
+      await this.setupTestEnvironment();
+      
+      // Launch browser
+      await this.launchBrowser();
+      
+      // Run priority test cases with fail-fast behavior
+      for (const testCase of TEST_CONFIG.testCases) {
+        try {
+          await this.runTestCase(testCase);
+        } catch (error) {
+          if (error.message.includes('CRITICAL:')) {
+            console.log('üõë CRITICAL ERROR - STOPPING ALL REMAINING TESTS');
+            break; // Stop processing remaining test cases
+          }
+        }
+      }
+      
+      // Generate report
+      await this.generateReport();
+      
+      console.log('\nüéâ Test Suite Completed!');
+      
+    } catch (error) {
+      console.error('‚ùå Test Suite Failed:', error.message);
+      throw error;
+    } finally {
+      await this.cleanup();
+    }
+  }
+
+  async setupTestEnvironment() {
