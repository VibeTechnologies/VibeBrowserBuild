+    if (!extensionLoaded) {
+      console.log('‚ùå AI Extension not loaded - likely due to component manifest parsing error');
+      return false;
+    }
+    
+    // Check browser console for AI agent logs
+    const logs = await this.getBrowserLogs();
+    const aiAgentLogs = logs.filter(log => 
+      log.message.includes('AI Agent') || 
+      log.message.includes('ai_agent.js') ||
+      log.message.includes('Vibe AI Agent')
