+#!/usr/bin/env node
+
+/**
+ * Test script for Chat4 sidebar functionality
+ * Run with: node test-sidebar.js
+ */
+
+const puppeteer = require('puppeteer');
+const path = require('path');
+const fs = require('fs');
+
+// Colors for console output
+const colors = {
+  green: '\x1b[32m',
+  red: '\x1b[31m',
+  yellow: '\x1b[33m',
+  reset: '\x1b[0m'
+};
+
+async function runTests() {
+  console.log('🧪 Starting Chat4 Sidebar Tests...\n');
+  
+  const browser = await puppeteer.launch({
+    headless: false, // Set to true for CI
+    devtools: true,
+    args: ['--window-size=1200,800']
+  });
+
+  try {
+    // Test 1: Development Server
+    console.log('📋 Test 1: Development Server Tests');
+    await testDevServer(browser);
+    
+    // Test 2: Static Build
+    console.log('\n📋 Test 2: Static Build Tests');
+    await testStaticBuild(browser);
+    
+    // Test 3: Different Screen Sizes
+    console.log('\n📋 Test 3: Responsive Tests');
+    await testResponsive(browser);
+    
+    console.log('\n✅ All tests completed!');
+    
+  } catch (error) {
+    console.error(`${colors.red}❌ Test failed:${colors.reset}`, error);
+  } finally {
+    await browser.close();
+  }
+}
+
+async function testDevServer(browser) {
+  const page = await browser.newPage();
+  
+  try {
+    // Navigate to dev server
+    await page.goto('http://localhost:3000', { waitUntil: 'networkidle0' });
+    
+    // Test 1.1: Check default state
+    const sidebarVisible = await page.$('.sidebar') !== null;
+    logTest('Sidebar hidden by default', !sidebarVisible);
+    
+    // Test 1.2: Check welcome message
+    const welcomeText = await page.$eval('h2', el => el.textContent);
+    logTest('Welcome message correct', welcomeText === 'What is on your mind?');
+    
+    // Test 1.3: Check toggle button exists
+    const toggleButton = await page.$('button[title*="sidebar"]');
+    logTest('Toggle button exists', toggleButton !== null);
+    
+    // Test 1.4: Test toggle functionality
+    await page.click('button[title*="sidebar"]');
+    await page.waitForTimeout(500); // Wait for animation
+    
+    const sidebarAfterClick = await page.$('.sidebar');
+    logTest('Sidebar shows after toggle', sidebarAfterClick !== null);
+    
+    // Test 1.5: Toggle back
+    await page.click('button[title*="sidebar"]');
+    await page.waitForTimeout(500);
+    
+    const sidebarHiddenAgain = await page.$('.sidebar') === null;
+    logTest('Sidebar hides on second toggle', sidebarHiddenAgain);
+    
+  } catch (error) {
+    console.error('Dev server test error:', error);
+  } finally {
+    await page.close();
+  }
+}
+
+async function testStaticBuild(browser) {
+  const page = await browser.newPage();
+  
+  try {
+    // Check if static build exists
+    const htmlPath = path.join(__dirname, 'output', 'chat.html');
+    if (!fs.existsSync(htmlPath)) {
+      console.log(`${colors.yellow}⚠️  Static build not found. Run 'npm run build-static' first${colors.reset}`);
+      return;
+    }
+    
+    // Navigate to static build
+    await page.goto(`file://${htmlPath}`, { waitUntil: 'networkidle0' });
+    
+    // Wait for React to hydrate
+    await page.waitForTimeout(1000);
+    
+    // Test static build functionality
+    const toggleButton = await page.$('button svg');
+    logTest('Toggle button in static build', toggleButton !== null);
+    
+    // Test click handler
+    await page.evaluate(() => {
+      const button = document.querySelector('button');
+      if (button) {
+        button.click();
+      }
+    });
+    
+    await page.waitForTimeout(500);
+    
+    // Check if any JavaScript errors occurred
+    const errors = [];
+    page.on('console', msg => {
+      if (msg.type() === 'error') {
+        errors.push(msg.text());
+      }
+    });
+    
+    logTest('No JavaScript errors', errors.length === 0);
+    
+  } catch (error) {
+    console.error('Static build test error:', error);
+  } finally {
+    await page.close();
+  }
+}
+
+async function testResponsive(browser) {
+  const page = await browser.newPage();
+  
+  try {
+    await page.goto('http://localhost:3000', { waitUntil: 'networkidle0' });
+    
+    // Test 3.1: Wide screen (desktop)
+    await page.setViewport({ width: 1200, height: 800 });
+    await page.waitForTimeout(500);
+    
+    const sidebarOnDesktop = await page.$('.sidebar');
+    logTest('Sidebar hidden on desktop by default', sidebarOnDesktop === null);
+    
+    // Test 3.2: Narrow screen (mobile/sidepanel)
+    await page.setViewport({ width: 600, height: 800 });
+    await page.waitForTimeout(500);
+    
+    const sidebarOnMobile = await page.$('.sidebar');
+    logTest('Sidebar hidden on mobile', sidebarOnMobile === null);
+    
+    // Test 3.3: Toggle still works on mobile
+    await page.click('button[title*="sidebar"]');
+    await page.waitForTimeout(500);
+    
+    const sidebarVisibleOnMobile = await page.$('.sidebar');
+    logTest('Toggle works on mobile', sidebarVisibleOnMobile !== null);
+    
+  } catch (error) {
+    console.error('Responsive test error:', error);
+  } finally {
+    await page.close();
+  }
+}
+
+function logTest(description, passed) {
+  const status = passed ? `${colors.green}✓${colors.reset}` : `${colors.red}✗${colors.reset}`;
+  console.log(`  ${status} ${description}`);
+}
+
+// Check if dev server is running
+async function checkDevServer() {
+  try {
+    const response = await fetch('http://localhost:3000');
+    return response.ok;
+  } catch {
+    return false;
+  }
+}
+
+// Main execution
+(async () => {
+  const devServerRunning = await checkDevServer();
+  if (!devServerRunning) {
+    console.log(`${colors.yellow}⚠️  Dev server not running. Start it with 'npm run dev'${colors.reset}`);
+    console.log('Continuing with static build tests only...\n');
+  }
+  
+  await runTests();
+})();
