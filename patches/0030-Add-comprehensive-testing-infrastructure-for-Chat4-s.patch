+import { test, expect } from '@playwright/test';
+import path from 'path';
+
+const STATIC_HTML_PATH = path.join(__dirname, '../output/chat.html');
+
+test.describe('Chat4 Sidebar - Static Build', () => {
+  test.beforeEach(async ({ page }) => {
+    // Enable console logging
+    page.on('console', msg => console.log(`BROWSER: ${msg.text()}`));
+    page.on('pageerror', err => console.log(`PAGE ERROR: ${err.message}`));
+    
+    // Navigate to static build
+    await page.goto(`file://${STATIC_HTML_PATH}`);
+    
+    // Wait for potential React hydration
+    await page.waitForTimeout(2000);
+  });
+
+  test('should load static HTML successfully', async ({ page }) => {
+    await expect(page.locator('body')).toBeVisible();
+    
+    // Check for basic structure
+    const title = await page.textContent('h2');
+    expect(title).toBe('What is on your mind?');
+  });
+
+  test('should have toggle button present', async ({ page }) => {
+    const toggleButton = page.locator('button[title*="sidebar"]');
+    await expect(toggleButton).toBeVisible();
+    
+    // Log button details
+    const buttonHTML = await toggleButton.innerHTML();
+    console.log('Toggle button HTML:', buttonHTML);
+  });
+
+  test('should detect React hydration status', async ({ page }) => {
+    // Check if React is available globally
+    const reactAvailable = await page.evaluate(() => {
+      return typeof window.React !== 'undefined';
+    });
+    console.log('React globally available:', reactAvailable);
+
+    // Check for React fiber on DOM elements
+    const hasFiber = await page.evaluate(() => {
+      const button = document.querySelector('button[title*="sidebar"]');
+      if (!button) return false;
+      
+      const keys = Object.keys(button);
+      return keys.some(key => key.includes('Fiber') || key.includes('react'));
+    });
+    console.log('React Fiber attached to button:', hasFiber);
+
+    // Check for React DevTools
+    const reactDevTools = await page.evaluate(() => {
+      return typeof window.__REACT_DEVTOOLS_GLOBAL_HOOK__ !== 'undefined';
+    });
+    console.log('React DevTools hook present:', reactDevTools);
+  });
+
+  test('should test sidebar toggle functionality', async ({ page }) => {
+    // Find toggle button
+    const toggleButton = page.locator('button[title*="sidebar"]');
+    await expect(toggleButton).toBeVisible();
+
+    // Check initial state - sidebar should be hidden
+    const sidebarBefore = page.locator('[data-testid="sidebar"]');
+    const isVisibleBefore = await sidebarBefore.isVisible().catch(() => false);
+    console.log('Sidebar visible before click:', isVisibleBefore);
+
+    // Click the toggle button
+    await toggleButton.click();
+    await page.waitForTimeout(500); // Wait for potential animation
+
+    // Check if sidebar appears
+    const isVisibleAfter = await sidebarBefore.isVisible().catch(() => false);
+    console.log('Sidebar visible after click:', isVisibleAfter);
+
+    // The test should pass if clicking changes the state
+    expect(isVisibleBefore).not.toBe(isVisibleAfter);
+  });
+
+  test('should capture JavaScript errors and React warnings', async ({ page }) => {
+    const errors: string[] = [];
+    const warnings: string[] = [];
+
+    page.on('console', msg => {
+      if (msg.type() === 'error') {
+        errors.push(msg.text());
+      } else if (msg.type() === 'warning') {
+        warnings.push(msg.text());
+      }
+    });
+
+    page.on('pageerror', err => {
+      errors.push(`Page Error: ${err.message}`);
