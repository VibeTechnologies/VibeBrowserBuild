+#!/usr/bin/env node
+
+/**
+ * Debug script to analyze the static build and identify hydration issues
+ */
+
+const fs = require('fs');
+const path = require('path');
+
+console.log('🔍 Debugging Chat4 Static Build...\n');
+
+// Check if files exist
+const outputDir = './output';
+const files = ['chat.html', 'chat.css', 'chat.js'];
+
+console.log('1. Checking build output files:');
+files.forEach(file => {
+  const filePath = path.join(outputDir, file);
+  const exists = fs.existsSync(filePath);
+  const size = exists ? (fs.statSync(filePath).size / 1024).toFixed(1) + 'KB' : 'N/A';
+  console.log(`   ${exists ? '✅' : '❌'} ${file}: ${size}`);
+});
+
+// Analyze HTML structure
+console.log('\n2. Analyzing HTML structure:');
+const htmlPath = path.join(outputDir, 'chat.html');
+if (fs.existsSync(htmlPath)) {
+  const html = fs.readFileSync(htmlPath, 'utf8');
+  
+  console.log('   React elements:');
+  console.log('   - __next div:', html.includes('__next') ? '✅' : '❌');
+  console.log('   - React root div:', html.includes('id="__next"') || html.includes('data-reactroot') ? '✅' : '❌');
+  console.log('   - Toggle button:', html.includes('Show sidebar') || html.includes('Hide sidebar') ? '✅' : '❌');
+  
+  console.log('\n   Hydration markers:');
+  console.log('   - Next.js hydration data:', html.includes('__NEXT_DATA__') ? '✅' : '❌');
+  console.log('   - React hydration markers:', html.includes('data-react') ? '✅' : '❌');
+  
+  // Check for inline scripts
+  const scriptMatches = html.match(/<script[^>]*>(.*?)<\/script>/gs) || [];
+  console.log(`\n   Inline scripts: ${scriptMatches.length}`);
+  
+  // Look for React initialization
+  const hasReactInit = html.includes('React') || html.includes('ReactDOM') || html.includes('hydrate');
+  console.log('   React initialization code:', hasReactInit ? '✅' : '❌');
+  
+  // Check for Next.js specific code
+  const hasNextCode = html.includes('_next') || html.includes('next/');
+  console.log('   Next.js specific code:', hasNextCode ? '✅' : '❌');
+}
+
+// Analyze JavaScript bundle
+console.log('\n3. Analyzing JavaScript bundle:');
+const jsPath = path.join(outputDir, 'chat.js');
+if (fs.existsSync(jsPath)) {
+  const js = fs.readFileSync(jsPath, 'utf8');
+  
+  console.log('   React functionality:');
+  console.log('   - useState hook:', js.includes('useState') ? '✅' : '❌');
+  console.log('   - setSidebarHidden:', js.includes('setSidebarHidden') ? '✅' : '❌');
+  console.log('   - ReactDOM render:', js.includes('render') || js.includes('createRoot') ? '✅' : '❌');
+  
+  console.log('\n   Event handling:');
+  console.log('   - onClick events:', js.includes('onClick') ? '✅' : '❌');
+  console.log('   - Event listeners:', js.includes('addEventListener') ? '✅' : '❌');
+  
+  // Check for hydration code
+  const hasHydration = js.includes('hydrate') || js.includes('hydrateRoot');
+  console.log('   Hydration code:', hasHydration ? '✅' : '❌');
+  
+  // Check if our component code is present
+  const hasChatPageCode = js.includes('ChatPage') || js.includes('sidebarHidden');
+  console.log('   ChatPage component:', hasChatPageCode ? '✅' : '❌');
+}
+
+console.log('\n4. Recommendations:');
+if (!fs.existsSync(htmlPath)) {
+  console.log('   ❌ HTML file missing - run npm run build-static');
+} else {
+  const html = fs.readFileSync(htmlPath, 'utf8');
+  
+  if (!html.includes('__NEXT_DATA__')) {
+    console.log('   ⚠️  Missing Next.js hydration data');
+  }
+  
+  if (!html.includes('React') && !html.includes('ReactDOM')) {
+    console.log('   ❌ React not properly initialized in static build');
+    console.log('      → The static build may not be setting up React hydration');
+    console.log('      → This is likely why the toggle button doesn\'t work');
+  }
+  
+  if (html.includes('Show sidebar') || html.includes('Hide sidebar')) {
+    console.log('   ✅ Toggle button is present in HTML');
+    console.log('   ➡️  Issue is likely JavaScript hydration, not HTML structure');
+  }
+}
+
+console.log('\n5. Next Steps:');
+console.log('   1. Fix React hydration in static build');
+console.log('   2. Ensure Next.js properly exports interactive components');
+console.log('   3. Test with simplified React app if needed');
+console.log('   4. Consider alternative build approach for static deployment');
+
+console.log('\n🔍 Debug analysis complete!');
