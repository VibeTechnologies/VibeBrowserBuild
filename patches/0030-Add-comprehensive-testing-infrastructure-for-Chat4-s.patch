+import React from 'react';
+import { render, screen, fireEvent, waitFor } from '@testing-library/react';
+import '@testing-library/jest-dom';
+import { ChatPage } from './ChatPage';
+
+describe('ChatPage Sidebar Functionality', () => {
+  it('should hide sidebar by default', () => {
+    render(<ChatPage />);
+    
+    // Sidebar should not be visible
+    const sidebar = screen.queryByRole('complementary');
+    expect(sidebar).not.toBeInTheDocument();
+  });
+
+  it('should show correct welcome message without emoji', () => {
+    render(<ChatPage />);
+    
+    // Check welcome message
+    const welcomeMessage = screen.getByText('What is on your mind?');
+    expect(welcomeMessage).toBeInTheDocument();
+    
+    // Ensure no emoji is present
+    const allText = screen.getByText('What is on your mind?').textContent;
+    expect(allText).not.toMatch(/[💬🎉😊]/);
+  });
+
+  it('should have toggle button in header', () => {
+    render(<ChatPage />);
+    
+    // Toggle button should exist
+    const toggleButton = screen.getByRole('button', { name: /sidebar/i });
+    expect(toggleButton).toBeInTheDocument();
+  });
+
+  it('should toggle sidebar visibility when button is clicked', async () => {
+    render(<ChatPage />);
+    
+    // Get toggle button
+    const toggleButton = screen.getByRole('button', { name: /sidebar/i });
+    
+    // Initially, sidebar should not be visible
+    expect(screen.queryByTestId('sidebar')).not.toBeInTheDocument();
+    
+    // Click toggle button
+    fireEvent.click(toggleButton);
+    
+    // Sidebar should now be visible
+    await waitFor(() => {
+      expect(screen.getByTestId('sidebar')).toBeInTheDocument();
+    });
+    
+    // Click again to hide
+    fireEvent.click(toggleButton);
+    
+    // Sidebar should be hidden again
+    await waitFor(() => {
+      expect(screen.queryByTestId('sidebar')).not.toBeInTheDocument();
+    });
+  });
+
+  it('should hide sidebar on narrow screens', () => {
+    // Mock window width
+    global.innerWidth = 500;
+    global.dispatchEvent(new Event('resize'));
+    
+    render(<ChatPage />);
+    
+    // Even if we try to show sidebar, it should auto-hide on narrow screens
+    const toggleButton = screen.getByRole('button', { name: /sidebar/i });
+    fireEvent.click(toggleButton);
+    
+    // Verify sidebar is not visible (or auto-hidden)
+    // This depends on implementation
+  });
+});
+
+// Debug test to log component state
+describe('ChatPage Debug', () => {
+  it('should log component rendering details', () => {
+    const { container } = render(<ChatPage />);
+    
+    console.log('=== ChatPage Debug Info ===');
+    console.log('HTML structure:', container.innerHTML.substring(0, 500));
+    
+    // Find all buttons
+    const buttons = screen.getAllByRole('button');
+    console.log('Number of buttons:', buttons.length);
+    buttons.forEach((btn, i) => {
+      console.log(`Button ${i}:`, btn.getAttribute('title'), btn.textContent);
+    });
+    
+    // Check for sidebar-related elements
+    const sidebarElements = container.querySelectorAll('[class*="sidebar"]');
+    console.log('Elements with sidebar class:', sidebarElements.length);
+  });
+});
