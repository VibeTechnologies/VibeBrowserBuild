+            const [sidebarHidden, setSidebarHidden] = useState(true);
+
+            // Auto-hide sidebar on narrow screens
+            useEffect(() => {
+                const checkWidth = () => {
+                    if (window.innerWidth < 1024) {
+                        setSidebarHidden(true);
+                    }
+                };
+                
+                checkWidth();
+                window.addEventListener('resize', checkWidth);
+                return () => window.removeEventListener('resize', checkWidth);
+            }, []);
+
+            const currentSession = sessions.find(s => s.id === currentSessionId);
+            const messages = currentSession?.messages || [];
+
+            const generateId = () => Math.random().toString(36).substring(2);
+
+            const createNewSession = () => ({
+                id: generateId(),
+                title: "New Chat",
+                lastMessage: "",
+                timestamp: new Date(),
+                messages: []
+            });
+
+            const handleNewChat = () => {
+                const newSession = createNewSession();
+                setSessions(prev => [newSession, ...prev]);
+                setCurrentSessionId(newSession.id);
+            };
+
+            const handleSelectSession = (sessionId) => {
+                setCurrentSessionId(sessionId);
+            };
+
+            const handleDeleteSession = (sessionId) => {
+                setSessions(prev => prev.filter(s => s.id !== sessionId));
+                if (currentSessionId === sessionId) {
+                    const remainingSessions = sessions.filter(s => s.id !== sessionId);
+                    setCurrentSessionId(remainingSessions[0]?.id);
+                }
+            };
+
+            const handleSendMessage = () => {
+                if (!message.trim()) return;
+
+                let sessionId = currentSessionId;
+                if (!sessionId) {
+                    const newSession = createNewSession();
+                    setSessions(prev => [newSession, ...prev]);
+                    sessionId = newSession.id;
+                    setCurrentSessionId(sessionId);
+                }
+
+                const userMessage = {
+                    id: generateId(),
+                    content: message.trim(),
+                    role: "user",
+                    timestamp: new Date()
+                };
+
+                setSessions(prev => prev.map(session => 
+                    session.id === sessionId
+                        ? { 
+                            ...session, 
+                            messages: [...session.messages, userMessage],
+                            lastMessage: userMessage.content,
+                            title: session.messages.length === 0 ? userMessage.content.slice(0, 50) : session.title,
+                            timestamp: new Date()
+                        }
+                        : session
+                ));
+
+                setMessage("");
+
+                // Simulate AI response
+                setTimeout(() => {
+                    const aiMessage = {
+                        id: generateId(),
+                        content: `I understand you said: "${userMessage.content}". This is a simulated response.`,
+                        role: "assistant",
+                        timestamp: new Date()
+                    };
+
+                    setSessions(prev => prev.map(session => 
+                        session.id === sessionId
+                            ? { 
+                                ...session, 
+                                messages: [...session.messages, aiMessage],
+                                lastMessage: aiMessage.content.slice(0, 50),
+                                timestamp: new Date()
+                            }
+                            : session
+                    ));
+                }, 1000);
+            };
+
+            // Initialize with a default session if none exist
+            useEffect(() => {
+                if (sessions.length === 0) {
+                    handleNewChat();
+                }
+            }, []);
+
+            console.log('ChatPage render - sidebarHidden:', sidebarHidden);
+
+            return (
+                <div className="flex h-screen bg-white">
+                    {/* Sidebar */}
+                    {!sidebarHidden && (
+                        <Sidebar
+                            sessions={sessions}
+                            currentSessionId={currentSessionId}
+                            onNewChat={handleNewChat}
+                            onSelectSession={handleSelectSession}
+                            onDeleteSession={handleDeleteSession}
+                        />
+                    )}
+
+                    {/* Main Chat Area */}
+                    <div className="flex flex-1 flex-col">
+                        {/* Header with toggle button */}
+                        <div className="border-b bg-white p-3 flex items-center justify-between">
+                            <button
+                                onClick={() => {
+                                    console.log('Toggle clicked, current state:', sidebarHidden);
+                                    setSidebarHidden(!sidebarHidden);
+                                }}
+                                className="p-2 hover:bg-gray-100 rounded-md transition-colors"
+                                title={sidebarHidden ? "Show sidebar" : "Hide sidebar"}
+                            >
+                                <SidebarIcon />
+                            </button>
+                            <div className="text-sm font-medium text-gray-600">
+                                Chat
+                            </div>
+                            <div className="w-10"></div>
+                        </div>
+                        
+                        <MessageList messages={messages} />
+                        <ChatInput
+                            message={message}
+                            onMessageChange={setMessage}
+                            onSendMessage={handleSendMessage}
+                        />
+                    </div>
+                </div>
+            );
+        }
+
+        // Render the app
+        const container = document.getElementById('root');
+        const root = ReactDOM.createRoot(container);
+        root.render(<ChatPage />);
+
+        console.log('Standalone Chat4 loaded successfully');
+    </script>
+</body>
+</html>
