+@import "tailwindcss";
+
+/* Theme variables */
+:root {
+  /* Light theme (default) */
+  --bg-primary: #ffffff;
+  --bg-secondary: #f7f7f8;
+  --bg-tertiary: #ececec;
+  --text-primary: #212121;
+  --text-secondary: #666666;
+  --border-color: #e5e5e5;
+  --input-bg: #ffffff;
+  --input-border: #d1d5db;
+  --button-bg: #000000;
+  --button-text: #ffffff;
+  --message-user-bg: #f0f0f0;
+  --message-assistant-bg: #ffffff;
+}
+
+.dark {
+  /* Dark theme */
+  --bg-primary: #212121;
+  --bg-secondary: #171717;
+  --bg-tertiary: #2d2d2d;
+  --text-primary: #ececec;
+  --text-secondary: #b3b3b3;
+  --border-color: #2d2d2d;
+  --input-bg: #2d2d2d;
+  --input-border: #404040;
+  --button-bg: #ffffff;
+  --button-text: #000000;
+  --message-user-bg: #2d2d2d;
+  --message-assistant-bg: #212121;
+}
+
+/* Reset and base styles */
+* {
+  box-sizing: border-box;
+}
+
+body {
+  margin: 0;
+  padding: 0;
+  font-family: "SÃ¶hne", -apple-system, BlinkMacSystemFont, "Segoe UI", "Roboto", "Oxygen", "Ubuntu", "Cantarell", "Fira Sans", "Droid Sans", "Helvetica Neue", sans-serif;
+  background-color: var(--bg-primary);
+  color: var(--text-primary);
+  line-height: 1.6;
+}
+
+#root {
+  width: 100%;
+  height: 100vh;
+  display: flex;
+  flex-direction: column;
+}
+
+/* Main chat container */
+.chat-container {
+  display: flex;
+  height: 100vh;
+  background-color: var(--bg-primary);
+}
+
+/* Sidebar */
+.chat-sidebar {
+  width: 260px;
+  background-color: var(--bg-secondary);
+  border-right: 1px solid var(--border-color);
+  display: flex;
+  flex-direction: column;
+  padding: 0.5rem;
+}
+
+/* Main chat area */
+.chat-main {
+  flex: 1;
+  display: flex;
+  flex-direction: column;
+  position: relative;
+}
+
+/* Messages area */
+.chat-messages {
+  flex: 1;
+  overflow-y: auto;
+  display: flex;
+  flex-direction: column;
+  align-items: center;
+  padding: 0;
+  max-width: 100%;
+}
+
+/* Welcome state */
+.welcome-container {
+  display: flex;
+  flex-direction: column;
+  align-items: center;
+  justify-content: center;
+  height: 100%;
+  max-width: 768px;
+  margin: 0 auto;
+  padding: 2rem;
+  text-align: center;
+}
+
+.welcome-title {
+  font-size: 2rem;
+  font-weight: 600;
+  color: var(--text-primary);
+  margin-bottom: 2rem;
+}
+
+/* Message styling */
+.message {
+  width: 100%;
+  max-width: 768px;
+  margin: 0 auto;
+  padding: 1.5rem 1rem;
+}
+
+.message.user {
+  background-color: var(--message-user-bg);
+}
+
+.message.assistant {
+  background-color: var(--message-assistant-bg);
+}
+
+.message-content {
+  font-size: 1rem;
+  line-height: 1.7;
+  color: var(--text-primary);
+}
+
+/* Input container */
+.chat-input-container {
+  position: relative;
+  padding: 1rem;
+  background-color: var(--bg-primary);
+  border-top: 1px solid var(--border-color);
+  display: flex;
+  justify-content: center;
+}
+
+.chat-input-wrapper {
+  position: relative;
+  width: 100%;
+  max-width: 768px;
+}
+
+/* Input styling */
+.chat-input {
+  width: 100%;
+  min-height: 52px;
+  padding: 12px 48px 12px 16px;
+  background-color: var(--input-bg);
+  border: 1px solid var(--input-border);
+  border-radius: 12px;
+  color: var(--text-primary);
+  font-size: 1rem;
+  line-height: 1.5;
+  resize: none;
+  outline: none;
+  transition: all 0.2s ease;
+  font-family: inherit;
+}
+
+.chat-input:focus {
+  border-color: #10a37f;
+  box-shadow: 0 0 0 2px rgba(16, 163, 127, 0.1);
+}
+
+.chat-input::placeholder {
+  color: var(--text-secondary);
+}
+
+/* Send button */
+.send-button {
+  position: absolute;
+  right: 8px;
+  top: 50%;
+  transform: translateY(-50%);
+  width: 32px;
+  height: 32px;
+  background-color: #10a37f;
+  border: none;
+  border-radius: 8px;
+  color: white;
+  cursor: pointer;
+  display: flex;
+  align-items: center;
+  justify-content: center;
+  transition: background-color 0.2s ease;
+}
+
+.send-button:hover {
+  background-color: #0d8f6f;
+}
+
+.send-button:disabled {
+  background-color: #2d2d2d;
+  cursor: not-allowed;
+}
+
+/* Buttons */
+.btn {
+  padding: 0.75rem 1rem;
+  border-radius: 8px;
+  border: 1px solid #4d4d4d;
+  cursor: pointer;
+  font-size: 0.875rem;
+  font-weight: 500;
+  transition: all 0.2s ease;
+  background-color: transparent;
+  color: #ececec;
+}
+
+.btn:hover {
+  background-color: #2d2d2d;
+  border-color: #6d6d6d;
+}
+
+.btn-primary {
+  background-color: #10a37f;
+  border-color: #10a37f;
+  color: white;
+}
+
+.btn-primary:hover {
+  background-color: #0d8f6f;
+  border-color: #0d8f6f;
+}
+
+/* Scrollbar styling */
+.chat-messages::-webkit-scrollbar {
+  width: 6px;
+}
+
+.chat-messages::-webkit-scrollbar-track {
+  background: transparent;
+}
+
+.chat-messages::-webkit-scrollbar-thumb {
+  background: #4d4d4d;
+  border-radius: 3px;
+}
+
+.chat-messages::-webkit-scrollbar-thumb:hover {
+  background: #6d6d6d;
+}
+
+/* Loading animation */
+.typing-indicator {
+  display: flex;
+  align-items: center;
+  gap: 4px;
+  padding: 1rem 0;
+}
+
+.typing-dot {
+  width: 8px;
+  height: 8px;
+  background-color: #8e8ea0;
+  border-radius: 50%;
+  animation: typing 1.4s infinite ease-in-out;
+}
+
+.typing-dot:nth-child(1) { animation-delay: -0.32s; }
+.typing-dot:nth-child(2) { animation-delay: -0.16s; }
+
+@keyframes typing {
+  0%, 80%, 100% {
+    transform: scale(0.8);
+    opacity: 0.5;
+  }
+  40% {
+    transform: scale(1);
+    opacity: 1;
+  }
+}
