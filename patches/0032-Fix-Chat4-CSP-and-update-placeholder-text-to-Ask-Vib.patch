+#!/usr/bin/env node
+
+const fs = require('fs');
+const path = require('path');
+const { execSync } = require('child_process');
+const esbuild = require('esbuild');
+
+// Configuration
+const OUTPUT_DIR = process.env.OUTPUT_DIRECTORY || './output';
+const SRC_DIR = './src';
+
+console.log('ðŸš€ Building Chat4 static interface...');
+
+// Clean and setup directories
+function setupDirectories() {
+  console.log('ðŸ“ Setting up directories...');
+  
+  // Clean output directory
+  if (fs.existsSync(OUTPUT_DIR)) {
+    fs.rmSync(OUTPUT_DIR, { recursive: true, force: true });
+  }
+  fs.mkdirSync(OUTPUT_DIR, { recursive: true });
+  
+  console.log(`âœ… Output directory ready: ${OUTPUT_DIR}`);
+}
+
+// Build CSS with Tailwind
+function buildCSS() {
+  console.log('ðŸŽ¨ Building CSS with Tailwind...');
+  
+  try {
+    const inputCss = path.join(SRC_DIR, 'styles/main.css');
+    
+    // Use Tailwind CLI to build CSS
+    execSync(`npx @tailwindcss/cli -i ${inputCss} -o ${OUTPUT_DIR}/chat.css --minify`, {
+      stdio: 'pipe',
+      cwd: __dirname
+    });
+    
+    const cssSize = fs.statSync(path.join(OUTPUT_DIR, 'chat.css')).size;
+    console.log(`âœ… CSS built: ${(cssSize / 1024).toFixed(1)}KB`);
+  } catch (error) {
+    console.error('âŒ CSS build failed:', error.message);
+    throw error;
+  }
+}
+
+// Build JavaScript with esbuild
+async function buildJavaScript() {
+  console.log('ðŸ“¦ Building JavaScript with esbuild...');
+  
+  try {
+    const entryFile = path.join(SRC_DIR, 'index.tsx');
+    
+    // Build with esbuild
+    await esbuild.build({
+      entryPoints: [entryFile],
+      bundle: true,
+      outfile: path.join(OUTPUT_DIR, 'chat.js'),
+      format: 'iife',
+      target: 'es2020',
+      minify: true,
+      sourcemap: false,
+      jsx: 'automatic',
+      alias: {
+        '@': path.resolve(SRC_DIR)
+      },
+      define: {
+        'process.env.NODE_ENV': '"production"'
+      },
+      external: [], // Bundle everything
+      loader: {
+        '.tsx': 'tsx',
+        '.ts': 'ts',
+        '.jsx': 'jsx',
+        '.js': 'js',
+        '.css': 'empty' // Ignore CSS imports since we handle CSS separately
+      }
+    });
+    
+    const jsSize = fs.statSync(path.join(OUTPUT_DIR, 'chat.js')).size;
+    console.log(`âœ… JavaScript built: ${(jsSize / 1024).toFixed(1)}KB`);
+  } catch (error) {
+    console.error('âŒ JavaScript build failed:', error.message);
+    throw error;
+  }
+}
+
+// Build HTML template
+function buildHTML() {
+  console.log('ðŸ“ Building HTML template...');
+  
+  const templateFile = path.join(SRC_DIR, 'template.html');
+  const htmlContent = fs.readFileSync(templateFile, 'utf8');
+  
+  fs.writeFileSync(path.join(OUTPUT_DIR, 'chat.html'), htmlContent);
+  
+  const htmlSize = fs.statSync(path.join(OUTPUT_DIR, 'chat.html')).size;
+  console.log(`âœ… HTML built: ${(htmlSize / 1024).toFixed(1)}KB`);
+}
+
+// Main build function
+async function build() {
+  try {
+    const startTime = Date.now();
+    
+    setupDirectories();
+    buildCSS();
+    await buildJavaScript();
+    buildHTML();
+    
+    const buildTime = Date.now() - startTime;
+    
+    console.log('\nðŸŽ‰ Build completed successfully!');
+    console.log(`â±ï¸  Build time: ${buildTime}ms`);
+    console.log('\nðŸ“„ Output files:');
+    
+    const files = ['chat.html', 'chat.js', 'chat.css'];
+    files.forEach(file => {
+      const filePath = path.join(OUTPUT_DIR, file);
+      if (fs.existsSync(filePath)) {
+        const size = fs.statSync(filePath).size;
+        console.log(`   ${file}: ${(size / 1024).toFixed(1)}KB`);
+      }
+    });
+    
+  } catch (error) {
+    console.error('\nâŒ Build failed:', error.message);
+    process.exit(1);
+  }
+}
+
+// Run build if called directly
+if (require.main === module) {
+  build();
+}
+
+module.exports = { build };
