+    
+    // Check for new elements
+    if (expectedChanges.newElements) {
+      expectedChanges.newElements.forEach(selector => {
+        const elements = querySelectorAllDeep(document, selector);
+        if (elements.length > 0) {
+          changeCount++;
+          findings.push(`‚úÖ New element appeared: ${selector} (${elements.length} found)`);
+        } else {
+          findings.push(`‚ùå Expected element not found: ${selector}`);
+        }
